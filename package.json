{
  "name": "specteam-vite-ui",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "author": "STRM",
  "license": "MIT",
  "repository": "git@github.com:waether-the-strm/makieta-specteam.git",
  "scripts": {
    "dev": "vite & npm run watch:css",
    "build": "npm run build:css && tsc && vite build",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "build:static": "npm run build && npm run build:server && node --experimental-json-modules scripts/generate-static.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:css": "tailwindcss -i ./src/index.css -o ./dist/output.css",
    "watch:css": "tailwindcss -i ./src/index.css -o ./dist/output.css --watch",
    "check:images": "node --experimental-json-modules .scripts/check-images.js",
    "get:dimensions": "node --experimental-json-modules .scripts/get-image-dimensions.js"
  },
  "dependencies": {
    "framer-motion": "^11.0.3",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "vite-plugin-compression": "^0.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "8.27.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "9.23.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "16.0.0",
    "postcss": "^8.4.35",
    "postcss-import": "^16.1.0",
    "sharp": "^0.33.2",
    "stylelint": "^16.11.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "8.27.0",
    "vite": "^5.4.2"
  }
}
